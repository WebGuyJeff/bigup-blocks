{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "bigup-blocks/anchor",
	"version": "0.1.0",
	"title": "Anchor",
	"category": "design",
	"description": "An anchor element.",
	"supports": {
		"html": false
	},
	"textdomain": "bigup-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"anchorClasses": {
			"type": "string",
			"enum": [ "bigupAnchor", "bigupAnchor bigupAnchor-full", "bigupAnchor bigupAnchor-narrow" ],
			"default": "bigupAnchor"
		}
	}
}
